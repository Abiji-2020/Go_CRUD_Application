{
  "name": "Go + Redis Dev Environment",
  "dockerComposeFile": "docker-compose.yml",
  "service": "app",
  "workspaceFolder": "/workspace",
  "forwardPorts": [8080, 6379],
  "customizations": {
    "vscode": {
      "settings": {
        "go.formatTool": "goimports",
        "go.lintTool": "golint",
        "go.useLanguageServer": true
      },
      "extensions": [
        "golang.go"
      ]
    }
  }
}
